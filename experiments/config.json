{
  "dataset": {
    "path": "dataset/input_data/5c_dlm_training_data_aggregated.csv",
    "brand": "absolut",
    // "brand_list": ["absolut", "chivas_regal", "havana_club", "jameson", "lillet", "malfy", "malibu", "ramazzotti"],
    "brand_list": ["havana_club", "jameson", "lillet", "malfy", "malibu", "ramazzotti"],
    "dependent_variable": "volume_so",
    "independent_variables_X": ["january", "february", "march", "april", "may", "june", "july", "august", "september", 
    "october", "november", "december", "is_one_week_before_last", "is_last_week_of_the_year", "is_christmas","is_easter"],
    "independent_variables_Z": ["television","out_of_home", "digital", "digital_social_media", "brand_experience",
    "off_trade_visibility", "distribution_off","relative_gap_to_90th_price", "pct_price_diff_with_competitors"],
    "isNormalizeX": false
  },
  "inferenceMethod": "torch_autograd", 
  "modelTraining": {
    "epoch": 15000,
    "learningRate": 0.0005,
    "momentum": 0.635068,
    "weightDecay": 1e-4,
    "maxGradNorm": 0.5,
    "maxGradValue": 0.1,
    "gammal2Lambda": 1e-2,
    "zetal2Lambda": 1e-2,
    "l2Lambda": 1e-2,
    "originalG": 0.0, 
    "addSigmoid": true,
    "minDelta": 1e-4,
    "factor": 0.4102291217861174,
    "patience": 10,
    "ifCrossValidation": true,
    "ifSimulation": false,
    "numTrainingData": 240,
    "numKFolder":5,
    "modelPath": "saved_models/model.pkl",
    "lossPath": "dataset/generated_results/loss_results.csv",
    "parametersPath": "dataset/generated_results/parameters_results.csv"
  },
  "simulationRecovery":{
    "independentRun": 3,
    "independentRunList": [2, 5, 10, 50],
    "ListG":  [4.5951, 5.29, 6.91],
    "paramsSavedPath": "dataset/generated_results/simulated_parameters.csv",
    "simulatedParamSavedPath": "dataset/generated_results/temp_simulated.csv",
    "isGammaEqualZeta": false
  },
  "inferenceParams":{
    "mcmc":{
      "nWalkers": 32,
      "numSamples": 1000,
      "burnInSteps": 100,
      "numStepsBetweenSamples": 50,
      "perturbationNoise": 0.0001
    }
  },
  "device": "cpu"
